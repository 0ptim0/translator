{
    "configurations": [
        {
            "name": "tranlsator",
            "includePath": [
                "${config:nuttx_path}/**",
                "${workspaceRoot}/**",
                "${workspaceFolder}/lib/libyaml/include",
                "${workspaceFolder}/src",
                "${workspaceFolder}/src/Interfaces",
                "${workspaceFolder}/src/RingBuffer",
                "${workspaceFolder}/tools/probe"
            ],
            "cStandard": "c99",
            "intelliSenseMode": "gcc-x64",
            "browse": {
                "path": [
                    "${workspaceFolder}, ${config:nuttx_path}"
                ],
                "limitSymbolsToIncludedHeaders": true,
                "databaseFilename": ""
            },
            "cppStandard": "c++11"
        }
    ],
    "version": 4
}